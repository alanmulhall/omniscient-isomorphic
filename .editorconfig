# http://editorconfig.org
root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
max_line_length = 80
trim_trailing_whitespace = true

[*.html]
indent_size = 4
indent_style = tab

[*.ejs]
indent_size = 4
indent_style = tab

[*.md]
max_line_length = 0
trim_trailing_whitespace = false
